<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_loan_application" model="ir.sequence">
            <field name="name">Loan Application Sequence</field>
            <field name="code">loan.application.sequence</field>
            <field name="prefix">APL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_loan_installment" model="ir.sequence">
            <field name="name">Loan Installment Sequence</field>
            <field name="code">loan.installment.sequence</field>
            <field name="prefix">INS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    
</odoo>